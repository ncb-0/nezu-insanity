{
	"redirects": [
		{
			"source": "/work/:path",
			"destination": "/:path"
		}
	],
	"headers": [
		{
			"source": "/:path",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=60, stale-while-revalidate=120"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "s-maxage=60, stale-while-revalidate=120"
				}
			]
		}
	]
}
